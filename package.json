{
  "name": "text-store",
  "version": "1.0.3",
  "description": "Super performant text container built upon caches",
  "main": "lib/index.js",
  "typings": "lib/typings/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "docs": "npm run _generate-docs & npm run _publish-docs",
    "_generate-docs": "typedoc --mode file --excludePrivate --out ./docs ./src",
    "_publish-docs": "gh-pages -d docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chipto/text-store.git"
  },
  "keywords": [
    "text",
    "string",
    "buffer",
    "manipulation",
    "util"
  ],
  "author": "Neek Sandhu <neek.sandhu@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chipto/text-store/issues"
  },
  "homepage": "https://github.com/chipto/text-store#readme",
  "devDependencies": {
    "gh-pages": "^1.1.0",
    "typedoc": "^0.9.0",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "clamp": "^1.0.1"
  }
}
